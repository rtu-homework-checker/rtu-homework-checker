<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.9.xsd">
    <!--
        Added the constraints for entity Score.
    -->
    <changeSet id="20201101180223-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="file_id"
                                 baseTableName="score"
                                 constraintName="fk_score_file_id"
                                 referencedColumnNames="id"
                                 referencedTableName="file"/>

        <addForeignKeyConstraint baseColumnNames="variant_id"
                                 baseTableName="score"
                                 constraintName="fk_score_variant_id"
                                 referencedColumnNames="id"
                                 referencedTableName="variant"/>

    </changeSet>
</databaseChangeLog>
