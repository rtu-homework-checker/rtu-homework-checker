<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.9.xsd">
    <!--
        Added the constraints for entity Scenarioscore.
    -->
    <changeSet id="20201101180123-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="score_id"
                                 baseTableName="scenarioscore"
                                 constraintName="fk_scenarioscore_score_id"
                                 referencedColumnNames="id"
                                 referencedTableName="score"/>

        <addForeignKeyConstraint baseColumnNames="scenarioexpectedoutput_id"
                                 baseTableName="scenarioscore"
                                 constraintName="fk_scenarioscore_scenarioexpectedoutput_id"
                                 referencedColumnNames="id"
                                 referencedTableName="scenarioexpectedoutput"/>

    </changeSet>
</databaseChangeLog>
